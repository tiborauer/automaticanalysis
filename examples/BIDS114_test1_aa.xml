<?xml version="1.0" encoding="utf-8"?>
<!-- This is a parameters and settings customising file to process BIDS multimodal test dataset ds114_test1 (https://googledrive.com/host/0B2JWN60ZLkgkMEw4bW5VUUpSdFU/ds114_test1.tar) -->
<aap>
    <options>
        <wheretoprocess>matlab_pct</wheretoprocess>
        <aaparallel>
            <numberofworkers>4</numberofworkers>
        </aaparallel>
        <NIFTI4D>1</NIFTI4D>
        <email>tibor.auer@rhul.ac.uk</email>
        
        <autoidentifystructural_choosefirst>1</autoidentifystructural_choosefirst>
    </options>

    <acq_details>
        <numdummies>0</numdummies>
        <input>
            <isBIDS>1</isBIDS>
            <combinemultiple>1</combinemultiple>
        </input>
    </acq_details>
        
    <tasksettings>
        <aamod_dartel_norm_write>
            <vox>1</vox>
        </aamod_dartel_norm_write>
        <aamod_diffusion_bet>
            <bet_f_parameter>0.4</bet_f_parameter>
        </aamod_diffusion_bet>
        <aamod_slicetiming>
            <autodetectSO>1</autodetectSO>
            <refslice>16</refslice>
        </aamod_slicetiming>
        <aamod_norm_write_dartel>
            <vox>3 3 3</vox>
        </aamod_norm_write_dartel>
        <aamod_smooth>
            <FWHM>5</FWHM>
        </aamod_smooth>
        <aamod_firstlevel_model>
            <includemovementpars>1</includemovementpars>
        </aamod_firstlevel_model>
        <aamod_firstlevel_threshold>
            <overlay><nth_slice>9</nth_slice></overlay>
        </aamod_firstlevel_threshold>
    </tasksettings>
    
    <firstlevel_contrasts>
        <!-- Contrast for test1 -->
        <con1>
            <subject>*</subject>
            <format>singlesession:fingerfootlips</format>
            <vector>1 0 0</vector>
            <name>Loc_Finger</name>
            <type>T</type>
        </con1>
        <con2>
            <subject>*</subject>
            <format>singlesession:fingerfootlips</format>
