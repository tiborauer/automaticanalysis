        <svd_tol>1e-6</svd_tol>
        
        <inputstreams>
            <stream isrenameable='1'>normalised_grey</stream>
        </inputstreams>
        <outputstreams>
            <stream isrenameable='1'>roidata</stream>
        </outputstreams>
   </currenttask>
  </tasklist>
