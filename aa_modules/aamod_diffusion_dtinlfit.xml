<?xml version="1.0" encoding="utf-8"?>
<aap>
    <tasklist>
        <currenttask domain='diffusion_session' desc='Run nonlinear DTI fit (by Marta Correia)' modality='MRI'>
            
            <qsub>
                <timeBase>0.5</timeBase>
                <memoryBase>0.1</memoryBase>
            </qsub>
            
            <permanenceofoutput>1</permanenceofoutput>
                        
            <inputstreams>
                <stream>bvals</stream>
                <stream>bvecs</stream>
                <stream>diffusion_data</stream>
                <stream>BETmask</stream>
            </inputstreams>
            
            <outputstreams>
                <stream>dti_FA</stream>
                <stream>dti_AD</stream>
                <stream>dti_RD</stream>
                <stream>dti_MD</stream>
                <stream>dti_L1</stream>
                <stream>dti_L2</stream>
                <stream>dti_L3</stream>
                <stream>dti_V1</stream>
                <stream>dti_V2</stream>
                <stream>dti_V3</stream>
                <stream>dti_S0</stream>
            </outputstreams>
            
        </currenttask>
    </tasklist>
</aap>
